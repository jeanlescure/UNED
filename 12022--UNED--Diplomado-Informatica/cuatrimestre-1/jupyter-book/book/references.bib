   issue = {2},
   journal = {New Zealand Journal of Geology and Geophysics},
   title = {New zealand radiocarbon age measurements — 6},
   volume = {14},
   year = {1971},
}
@article{Hua2021,
   abstract = {This paper presents a compilation of atmospheric radiocarbon for the period 1950-2019, derived from atmospheric CO2 sampling and tree rings from clean-air sites. Following the approach taken by Hua et al. (2013), our revised and extended compilation consists of zonal, hemispheric and global radiocarbon (14C) data sets, with monthly data sets for 5 zones (Northern Hemisphere zones 1, 2, and 3, and Southern Hemisphere zones 3 and 1-2). Our new compilation includes smooth curves for zonal data sets that are more suitable for dating applications than the previous approach based on simple averaging. Our new radiocarbon dataset is intended to help facilitate the use of atmospheric bomb 14C in carbon cycle studies and to accommodate increasing demand for accurate dating of recent (post-1950) terrestrial samples.},
   author = {Quan Hua and Jocelyn C. Turnbull and Guaciara M. Santos and Andrzej Z. Rakowski and Santiago Ancapichún and Ricardo De Pol-Holz and Samuel Hammer and Scott J. Lehman and Ingeborg Levin and John B. Miller and Jonathan G. Palmer and Chris S.M. Turney},
   doi = {10.1017/RDC.2021.95},
   issn = {00338222},
   journal = {Radiocarbon},
   title = {ATMOSPHERIC RADIOCARBON for the PERIOD 1950-2019},
   year = {2021},
}
@book{Hardt2000,
   abstract = {Este libro esboza una utopía política. En otras palabras, lanza una mirada a los regímenes de explotación y control que caracterizan el nuevo orden mundial con el fin de crear una alternativa: una sociedad global autenticamente democrática. Hardt y Negri utilizan conceptos como los de soberanía, nación y pueblo para reflejar los cambios culturales y económicos propios de la sociedad posmoderna, con sus nuevas formas de racismo, nuevas concepciones de la identidad y la diferencia, nuevas redes de comunicación y control, y nuevos itinerarios de migración. También muestran en qué medida el poder de las empresas transnacionales y el creciente predominio de las formas postindustriales del trabajo y la producción contribuyen a definir el nuevo orden global imperial.},
   author = {Michael Hardt and Antonio Negri},
   publisher = {Barcelona: Paidós},
   title = {IMPERIO Michael Hardt - Antonio Negri},
   year = {2000},
}
